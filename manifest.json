{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Leads tracker 2.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "tabs"

    ]
}